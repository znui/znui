    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@zeanium/core": "^1.0.5",
    "@zeanium/web": "^1.0.5",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "raw-loader": "^1.0.0",
